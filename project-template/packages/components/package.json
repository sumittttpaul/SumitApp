{
  "name": "@packages/components",
  "version": "1.0.0",
  "private": true,
  "strict": true,
  "exports": {
    "./*": "./src/*.tsx"
  },
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc --noEmit",
    "generate:component": "turbo gen react-component",
    "clean": "rimraf --glob .turbo dist node_modules tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@legendapp/state": "^3.0.0-beta.32",
    "@packages/hooks": "workspace:^",
    "@packages/utils": "workspace:^",
    "@packages/validations": "workspace:^",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@packages/eslint-config": "workspace:^",
    "@packages/types": "workspace:^",
    "@packages/typescript-config": "workspace:^",
    "@types/react": "~19.1.10",
    "@types/react-dom": "19.1.9",
    "eslint": "^9.35.0"
  }
}
