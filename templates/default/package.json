    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "turbo": "^2.5.8",
    "typescript": "~5.9.2"
  },
  "resolutions": {
    "expo-dev-menu": "^7.0.10"
  },
  "engines": {
    "node": ">=22"
  },
  "workspaces": [
    "projects/*",
    "packages/*"
