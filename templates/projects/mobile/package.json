    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-css": "^3.0.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-ios-context-menu": "3.1.0",
    "react-native-ios-utilities": "5.1.2",
    "react-native-pager-view": "6.9.1",
    "react-native-paper": "^5.14.5",
