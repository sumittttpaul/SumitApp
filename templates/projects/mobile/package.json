    "@packages/utils": "workspace:^",
    "@packages/validations": "workspace:^",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-menu/menu": "1.2.2",
    "@react-navigation/native": "^7.1.8",
    "core-js": "^3.45.1",
    "expo": "54.0.20",
    "expo-blur": "~15.0.6",
