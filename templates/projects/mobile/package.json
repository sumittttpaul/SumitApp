{
  "name": "mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "strict": true,
  "scripts": {
    "dev": "expo start --clear",
    "build": "expo prebuild --clean",
    "start": "expo start --no-dev --clear",
    "android": "expo run:android -d",
    "ios": "expo run:ios -d",
    "start:minify": "expo start --no-dev --minify --clear",
    "compatible": "expo install --fix",
    "doctor": "npx expo-doctor --verbose",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "clean": "rimraf --glob .turbo .expo android ios node_modules tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
